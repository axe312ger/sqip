{
  "name": "sqip",
  "version": "0.3.1",
  "description": "A SVG-based LQIP image placeholder generator",
  "main": "./dist/node.js",
  "bin": {
    "sqip": "./dist/cli.js"
  },
  "scripts": {
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:unit": "jest --testPathPattern=test/unit --coverage",
    "test:unit:watch": "jest --testPathPattern=test/unit --coverage --watch",
    "test:e2e": "jest --testPathPattern=test/e2e",
    "test:e2e:watch": "jest --testPathPattern=test/e2e --watch",
    "prepare": "npm run build-vendor",
    "build": "babel -d dist src",
    "build-vendor": "node scripts/bundle-vendor.js",
    "dev": "npm run build -- --watch",
    "lint": "eslint src scripts",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "files": [
    "dist",
    "vendor"
  ],
  "keywords": [
    "lqip",
    "svg",
    "images",
    "placeholder",
    "webperf",
    "performance",
    "ux",
    "perceived"
  ],
  "author": "Tobias Baldauf",
  "license": "Unlicense",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "cheerio": "^1.0.0-rc.2",
    "image-size": "^0.6.1",
    "svgo": "^0.7.2",
    "yargs": "^11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technopagan/sqip"
  },
  "person": {
    "name": "Tobias Baldauf",
    "email": "npm@tobias-baldauf.de",
    "url": "https://who.tobias.is/"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-prettier-standard": "^1.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "nixt": "^0.5.0",
    "prettier": "^1.11.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
